{
  "name": "@sst-openapi-boilerplate/api-client",
  "version": "1.0.0",
  "description": "API client",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "typescript": "tsc",
    "bundle-definition": "webpack",
    "openapi": "openapi read --json ../openapi.yml > src/openapi.json",
    "typegen": "echo '/* eslint-disable */' > src/openapi.d.ts && typegen ../openapi.yml >> src/openapi.d.ts",
    "build": "npm run openapi && npm run typegen && npm run typescript && npm run bundle-definition",
    "eslint": "../node_modules/.bin/eslint -c ../.eslintrc.yml",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "",
    "directory": "client"
  },
  "homepage": "",
  "files": [
    "*.js",
    "*.d.ts",
    "**/*.js",
    "**/*.d.ts",
    "!*.test.*",
    "!**/*.test.*",
    "!node_modules",
    "!src",
    "!*.config.js"
  ],
  "keywords": [],
  "license": "UNLICENSED",
  "dependencies": {
    "buffer": "^6.0.3",
    "https-browserify": "^1.0.0",
    "openapi-client-axios": "^7.5.1",
    "stream-http": "^3.1.1",
    "url": "^0.11.0",
    "util": "^0.12.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "axios": "^1.6.2",
    "copy-webpack-plugin": "^7.0.0",
    "json-loader": "^0.5.7",
    "openapi-client-axios-typegen": "^7.4.1",
    "openapicmd": "^2.7.0",
    "ts-loader": "^8.0.14",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.3",
    "webpack": "^5.18.0",
    "webpack-cli": "^4.4.0"
  }
}
