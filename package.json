{
  "name": "sst-openapi-boilerplate",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "sst dev",
    "start:swagger": "pnpm -r --filter ./packages/core --if-present run dev",
    "build:types": "pnpm -r --filter ./packages/core --if-present run build",
    "start:dev": "sst dev --stage dev",
    "start:stag": "sst dev --stage stag",
    "start:prod": "sst dev --stage prod",
    "deploy:dev": "sst deploy --stage dev",
    "deploy:tag": "sst deploy --stage stag",
    "deploy:rod": "sst deploy --stage prod"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^22.15.21",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "aws-cdk-lib": "2.212.0",
    "constructs": "10.2.69",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "sst": "^3.14.19",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.32.1"
  },
  "overrides": {
    "form-data": "^4.0.0",
    "tough-cookie": "^4.1.3",
    "tmp": "^0.2.4",
    "request": false,
    "request-promise-native": false,
    "request-promise-core": false
  },
  "workspaces": [
    "packages/*"
  ]
}